\pset null 'NULL'
\set AUTOCOMMIT off
\set ON_ERROR_ROLLBACK on
\set ON_ERROR_STOP on
\set HISTFILE ~/.local/psql_history/.psql_history- :DBNAME
\set HISTSIZE 500000
\timing
\encoding unicode
\pset linestyle unicode
-- \set ECHO_HIDDEN 1
-- red prompt for live
--\set PROMPT1 '%n@%m:%> [%/]%x=%# '
\set PROMPT1 '%[%033[0;32m%]%n%[%033[0m%]@%[%033[0;34m%]%m%[%033[0m%]:%[%033[0;35m%]%>%[%033[0m%] [%[%033[0;31m%]%/%[%033[0m%]]%[%033[0;33m%]%x%[%033[0m%]=%# '
--\set PROMPT2 '%n@%m:%> [%/]%x%R# '
\set PROMPT2 '%[%033[0;32m%]%n%[%033[0m%]@%[%033[0;34m%]%m%[%033[0m%]:%[%033[0;35m%]%>%[%033[0m%] [%[%033[0;31m%]%/%[%033[0m%]]%[%033[0;33m%]%x%[%033[0m%]%R%# '

